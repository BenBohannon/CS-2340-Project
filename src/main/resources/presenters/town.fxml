<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.GridPane?>

<GridPane prefHeight="800.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="presenters.TownPresenter">
  <children>
    <Button mnemonicParsing="false" prefHeight="250" prefWidth="250" text="ENERGY MULES" onAction="#handleMuleClick" GridPane.columnIndex="0" GridPane.rowIndex="0" />
    <Button mnemonicParsing="false" prefHeight="250" prefWidth="250" text="FOOD MULES" onAction="#handleMuleClick" GridPane.columnIndex="1" GridPane.rowIndex="0" />
    <Button mnemonicParsing="false" prefHeight="250" prefWidth="250" text="Pub" onAction="#handlePubClick" GridPane.columnIndex="2" GridPane.rowIndex="1" />
    <Button mnemonicParsing="false" prefHeight="250" prefWidth="250" text="SMITHORE MULES" onAction="#handleMuleClick" GridPane.columnIndex="0" GridPane.rowIndex="1" />
    <Button mnemonicParsing="false" prefHeight="250" prefWidth="250" text="CRYSTITE MULES" onAction="#handleMuleClick" GridPane.columnIndex="1" GridPane.rowIndex="1" />
    <Button mnemonicParsing="false" prefHeight="250" prefWidth="250" text="Map" onAction="#handleMapClick" GridPane.columnIndex="2" GridPane.rowIndex="0" />
  </children>
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
  </rowConstraints>
</GridPane>
